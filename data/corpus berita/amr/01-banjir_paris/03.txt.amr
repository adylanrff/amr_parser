     :ARG1 (vv2 / rabu
                :mod (vv3 / januari
                          :mod (vv4 / 24)
                          :mod (vv5 / 2018)))
     :ARG1 (vv6 / air
                :mod (vv7 / sungai
                          :name (vv8 / seine
                                     :name (vv9 / paris))))
     :mod (vv10 / telah)
