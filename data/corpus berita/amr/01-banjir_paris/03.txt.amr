                                    :mod (vv5 / prancis))))
     :mod (vv6 / telah)
     :location (vv7 / ancam
                    :mod (vv8 / banjir)
                    :ARG1 (vv9 / air
                               :mod (vv10 / sungai
                                          :name (vv11 / seine)
                                          :mod (vv12 / luap
                                                     :mod (vv13 / terus)))))
